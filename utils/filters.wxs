var completion = function(num) {
  return num > 10 ? num : '0' + num
}
var title = function(array){
  if(!array){
    return 
  }
  if(array.length = 1){
    return array[0]
  }else{
    return array.join('/')
  }
}
var muiscShape = function(type,className){
  if(type == 200){
    return className
  }
}
var sliceTag = function(array,length){
  return array.slice(0,length)
}
var fomatTxt = function(text){
  if(!text){
    return
  }
  var reg = getRegExp('\\\\n','g')
  return text.replace(reg,'\n&nbsp;&nbsp;&nbsp;&nbsp;')
}
module.exports = {
  completion: completion,
  title: title,
  sliceTag: sliceTag,
  fomatTxt: fomatTxt,
  muiscShape: muiscShape
}